      <bpmn2:script><![CDATA[float a =(float) kcontext.getVariable("nota1G");
float b =(float) kcontext.getVariable("nota2G");
float c =(float) kcontext.getVariable("nota3G");
float d =(float) kcontext.getVariable("nota4G");
float e =(float) kcontext.getVariable("nota5G");

float promedio= (float) (a+b+c+d+e)/5 ;


kcontext.setVariable("promedioG",promedio);
System.out.println("su promedio" + promedio);

if(promedio <= 0.29){
  
  System.out.println("su resultado: REPROBADO");
}
if(promedio >=3.0 && promedio <=3.9){
  
  System.out.println("su resultado: SUFICIENTE");
}
if(promedio >=4.0 && promedio <=4.8){
  
  System.out.println("su resultado: BIEN");
}
if(promedio >=4.9){
  
  System.out.println("su resultado: EXELENTE");
}]]></bpmn2:script>
